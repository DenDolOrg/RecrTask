<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/js")
    <title>@ViewBag.Title</title>

</head>
<body>
    <div id="container">
        
        <div id="header">
            <h1>
                @Html.ActionLink("Recruitment task", "Index", "Home")
            </h1>
            <div id="headerMenu">
                @Html.ActionLink("Upload file", "FileChoose", "Excel")
                @Html.ActionLink("Person list", "PersonList", "Person")
                @Html.ActionLink("Error list", "UploadErrors", "Person")
            </div>
        </div>
        <div id="body">
            @RenderBody()
        </div>
    </div>
</body>
</html>