<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="../Content/Site.css" type="text/css" rel="stylesheet">
    <script src="~/Scripts/jquery-3.3.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/mySkripts.js" type="text/javascript"></script>
    <title>@ViewBag.Title</title>

</head>
<body>
    <div id="container">
        <div id="header">
            <h1>
                @Html.ActionLink("Recruitment task", "PersonList", "Person")
            </h1>
            <div id="headerMenu">
                @Html.ActionLink("Upload file", "FileChoose", "Excel")
                @Html.ActionLink("Person list", "PersonList", "Person")
                @Html.ActionLink("Error list", "UploadErrors", "Person")
            </div>
        </div>
        <div id="body">
            @RenderBody()
        </div>
    </div>
</body>
</html>