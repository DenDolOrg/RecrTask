@model RekrutTask.Models.FullInfoViewModel

@{
    ViewBag.Title = "PersonFullInfo";
}
<table class="fullInfoTbl">
    @for (int i = 0; i < Model.PersonParamNames.Count(); i++)
    {
        <tr>
            <th>@Model.PersonParamNames.ElementAt(i)</th>
            <td>@Model.PersonParamValues.ElementAt(i)</td>
        </tr>
    }
 </table>

<hr>

<table class="fullInfoTbl">
    @for (int i = 0; i < Model.FinancialStateParamNames.Count(); i++)
    {
        <tr>
            <th>@Model.FinancialStateParamNames.ElementAt(i)</th>
            <td>@Model.FinancialStateParamValues.ElementAt(i)</td>
        </tr>
    }
</table>

@Html.ActionLink("Back", "PersonList", "PersonList", new {@class = "backLink" })

